<div class="home_left" style="margin-bottom: 5px;">
  <div class="home_top">
    <div class="content_home">
      <p>
        Phim của năm : <%= params[:id] %>
      </p>
    </div>
    <div class="home_frame">
      <div class="item_home_frame">
        <%= render "year_movies/item_year_movie" %>
      </div>
    </div>
    <div style="width: 100%; height: 40px;">
      <div style="width: 30%; margin:auto;">
        <%= paginate @movie, theme: "twitter-bootstrap-3", window: 3 %>
      </div>
    </div>
  </div>
</div>
<div class="home_right">
  <div class="home_top">
    <div class="content_home">
      <p>
        Phim sắp chiếu
      </p>
    </div>
    <div class="home_frame">
      <%= render "genre_movies/item_coming_soon" %>
    </div>
  </div>
</div>
<div style="clear: both;">

</div>
